{
  "name": "flask-react-redux-boilerplate",
  "description": "boilerplate for flask, react and redux",
  "version": "1.0.0",
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && npm run build:prod",
    "build:prod": "npm run clean && cross-env NODE_ENV=production webpack --config webpack/production.config.js",
    "build:dev": "npm run clean && cross-env NODE_ENV=development webpack --config webpack/development.config.js",
    "start": "node bin/run.js",
    "start:dev": "cross-env NODE_ENV=development node bin/run.js",
    "start:prod": "cross-env NODE_ENV=production node bin/run.js",
    "devserver:dev": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline --config webpack/development.config.js",
    "devserver:prod": "./node_modules/webpack-dev-server/bin/webpack-dev-server.js --hot --inline --config webpack/production.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sungminoh/flask-react-redux-boilerplate.git"
  },
  "author": "Sungmin Oh <smoh2044@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "eslint": "^4.4.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-babel": "^4.1.2",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "eventsource-polyfill": "^0.9.6",
    "extract-text-webpack-plugin": "^3.0.0",
    "react": "^15.6.1",
    "react-css-modules": "^4.6.0",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^1.3.1",
    "react-redux": "^5.0.5",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.1",
    "style-loader": "^0.18.2",
    "webpack": "^3.5.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-dev-server": "^2.6.1",
    "webpack-hot-middleware": "^2.18.2"
  },
  "dependencies": {
    "element-resize-detector": "^1.1.12",
    "es6-promise": "^4.1.1",
    "express": "^4.15.4",
    "http-proxy": "^1.16.2",
    "isomorphic-fetch": "^2.2.1"
  }
}
